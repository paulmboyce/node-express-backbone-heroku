<% layout('layout') -%>
<form action="/resetPassword" method="post">
  <input type="hidden" name="accountId" value="#{locals.accountId}"/>
  <p>
    <label for="password">New Password:</label>
    <input id="password" type="password" name="password"/>
  </p>
  <p>
    <input type="submit"/>
  </p>
</form>
